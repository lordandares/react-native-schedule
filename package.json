  "scripts": {
    "start": "react-native start",
    "start:clean": "rm -rf /tmp/metro-* && rm -rf /tmp/haste-map/metro-* && react-native start --reset-cache",
    "android": "react-native run-android",
    "android:log": "react-native log-android",
    "android:logcat": "adb logcat",
    "android:clean:run": "cd android/ && ./gradlew clean && cd .. && yarn android:uninstall && react-native run-android",
    "android:reverse": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9090 tcp:9090",
    "android:restart": "adb shell input keyevent KEYCODE_MENU && adb shell input keyevent 66 && adb shell input keyevent 66",
    "android:menu": "adb shell input keyevent 82",
    "android:screenshot": "mkdir -p screenshots && adb exec-out screencap -p > ./screenshots/$(date +\"%Y-%m-%d_%H-%M-%S\").png",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:uninstall": "adb shell pm clear com.NextTeamSoftware && adb uninstall com.NextTeamSoftware",
    "android:install:release": "yarn android:uninstall; adb install ./android/app/build/outputs/apk/release/app-release.apk",
    "android:install:debug": "yarn android:uninstall; adb install ./android/app/build/outputs/apk/debug/app-debug.apk && yarn android:run",
    "android:run": "adb shell am start -n com.NextTeamSoftware/.MainActivity",
    "android:clear": "adb shell pm clear com.NextTeamSoftware",
    "android:clean": "cd android/ && ./gradlew clean && cd ..",
    "killdevserver": "fuser -k 8081/tcp",
    "ios": "react-native run-ios",
    "ios:log": "react-native log-ios",
    "test": "jest --forceExit --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "lint": "yarn eslint",
    "eslint": "eslint --fix .",
    "flow": "flow",
    "test-debug": "./node_modules/.bin/node-debug --web-port=8182 ./node_modules/.bin/jest -i --runInBand --watch",
    "push-sourcemap": "node_modules/@sentry/cli/bin/sentry-cli     --auth-token c9bf82f48cc946aeb630dc32526757992c83b8dd654e42a3ab0d12835cc0bd57     releases     --org team-software     --project react-native-next     files com.NextTeamSoftware     upload-sourcemaps     --dist 1     --strip-prefix ./     --rewrite ./",
    "create-soucemap": "react-native bundle   --dev false   --platform android   --entry-file index.android.js   --bundle-output android.main.bundle   --sourcemap-output android.main.bundle.map",
    "clean": "rm -rf /tmp/metro-* &&  npm cache clean --force && text -x watchman && watchman watch-del-all"
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-with-styles|@next)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$": "<rootDir>/mocks/fileMock.js"
    }
  },
  "dependencies": {
    "@next/auth": "*",
