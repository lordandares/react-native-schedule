    "react-native-swiper": "^1.5.14",
    "react-native-user-avatar": "^1.0.3",
    "react-native-vector-icons": "^6.2.0",
    "react-redux": "^5.0.6",
    "react-with-styles": "^2.3.0",
    "react-with-styles-interface-react-native": "^1.0.0",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
